                    $('#contentNewFile').show();
                } else {
                    ifNew = true;
                }
            }).on('hide.bs.modal', function(){
                $('#contentNewFile').hide();
                $('#contentNewChromosome').hide();
                $('#contentNewGene').hide();
            });

            $("#checkstatus").on("shown.bs.modal", function () {
                if(ifNew){
                    $("#checkdialog").show();
                }else{
                    ifNew = true;
                }
            }).on("hide.bs.modal", function () {
                $("#checkdialog").hide();
            });


        })

        //打开新建基因窗体
